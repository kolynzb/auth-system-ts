  "scripts": {
    "build": "rimraf build && swc ./src -d build",
    "build-dev": "yarn run build && NODE_ENV=development node --inspect build/server.js",
    "dev": "nodemon --ext ts --exec \"yarn run build-dev\"",
    "start": "node build/server.js",
    "start:docker": "yarn build && node --inspect=0.0.0.0 build/server.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "volta": {
    "node": "16.16.0",
    "yarn": "1.22.19"
  },
