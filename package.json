    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7"
  },
